{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "keyPairId": {
      "type": "string",
      "maxLength": 1024,
      "pattern": "^([^<>{}]*)$"
    },
    "instanceOsUser": {
      "type": "string",
      "maxLength": 1024,
      "pattern": "^([^<>{}]*)$"
    }
  },
  "required": ["keyPairId"]
}
