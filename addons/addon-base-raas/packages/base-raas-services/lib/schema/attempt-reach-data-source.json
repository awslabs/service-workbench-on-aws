{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "pattern": "^([^<>{}]*)$"
    },
    "type": {
      "type": "string",
      "enum": ["dsAccount", "study"]
    },
    "status": {
      "type": "string",
      "enum": ["active", "inactive", "pending", "*"]
    }
  },
  "required": ["id"]
}
