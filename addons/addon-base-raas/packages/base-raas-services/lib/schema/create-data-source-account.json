{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 300
    },
    "description": {
      "type": "string",
      "maxLength": 2048
    },
    "contactInfo": {
      "type": "string",
      "maxLength": 2048
    },
    "id": {
      "type": "string",
      "pattern": "^[0-9]{12}$"
    },
    "type": {
      "type": "string",
      "enum": ["unmanaged", "managed-nonmember"]
    }
  },
  "required": ["name", "id", "type"]
}
