{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "pattern": "^[A-Za-z0-9-_]+$"
    },
    "description": {
      "type": "string",
      "maxLength": 3000
    },
    "indexId": {
      "type": "string"
    },
    "projectAdmins": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "username": {
              "type": "string",
              "minLength": 3
            },
            "ns": {
              "type": "string",
              "minLength": 3
            }
          }
        }
      ],
      "default": []
    }
  },
  "required": ["id", "indexId"]
}
