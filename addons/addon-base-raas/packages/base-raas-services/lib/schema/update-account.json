{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "pattern": "^[A-Za-z0-9-_]+$"
    },
    "stackId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "cfnInfo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "vpcId": {
          "type": "string",
          "pattern": "^vpc-[a-f0-9]{8,17}$"
        },
        "subnetId": {
          "type": "string",
          "pattern": "^subnet-[a-f0-9]{8,17}$"
        },
        "crossAccountExecutionRoleArn": {
          "type": "string",
          "pattern": "^arn:aws:iam::.*$"
        },
        "stackId": {
          "type": "string"
        },
        "encryptionKeyArn": {
          "type": "string",
          "pattern": "^arn:aws:kms:.*$"
        }
      }
    },
    "status": {
      "type": "string",
      "maxLength": 2048
    },
    "name": {
      "type": "string",
      "maxLength": 2048
    }
  },
  "required": ["id"]
}
