{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "pattern": "^[A-Za-z0-9-_ ]+$"
    },
    "description": {
      "type": "string",
      "maxLength": 2048
    },
    "userType": {
      "type": "string",
      "enum": [
        "INTERNAL", "EXTERNAL"
      ]
    }
  },
  "required": [
    "id",
    "userType"
  ]
}
