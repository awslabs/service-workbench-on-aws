{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "DisplayName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "Description": {
      "type": "string",
      "maxLength": 300
    },
    "ProviderName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    }
  },
  "required": ["DisplayName", "ProviderName"]
}
