{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 200,
      "pattern": "^[A-Za-z0-9-_ ]+$"
    },
    "rev": {
      "type": "number",
      "minimum": 0
    },
    "triggerType": {
      "type": "string"
    },
    "triggerTypeData": {
      "type": "string"
    },
    "wf": {
      "type": "string"
    }
  },
  "required": ["id", "rev", "triggerType", "triggerTypeData", "wf"]
}
