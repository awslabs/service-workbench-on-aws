{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 3,
      "maxLength": 300,
      "pattern": "^[A-Za-z0-9-_]+$"
    },
    "password": {
      "type": "string",
      "minLength": 4,
      "maxLength": 2048
    }
  },
  "required": [ "username", "password"]
}
